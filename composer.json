{
    "name": "adesubomi/larastack",
    "description": "A Simple and Efficient Wrapper Around Paystack API for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oniya Daniel",
            "email": "daniel.prince84@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Adesubomi\\Larastack\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adesubomi\\Larastack\\Provider\\LarastackServiceProvider"
            ],
            "aliases": {
                "Larastack": "Adesubomi\\Larastack\\Facade\\LarastackFacade"
            }
        }
    }
}
